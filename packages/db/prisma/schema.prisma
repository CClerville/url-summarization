generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model UrlSummary {
  id        String   @id @default(cuid())
  url       String
  summary   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([createdAt])
  @@index([url])
}
